<!DOCTYPE html>
<html>
<head>
  <title>Admin - Meshiterro</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>
<body>
  <% if admin_signed_in? %>
    <div class="text-right">
      <%= link_to 'ログアウト', posts_path %>
    </div>
    <div class="text-right">
      <%= link_to 'コメント一覧', admin_comments_path %>
    </div>
    <div class="text-right">
      <%= link_to '全ユーザー一覧', admin_dashboards_path %>
    </div>
  <% end %>
  <%= yield %>
</body>
</html>